{
    "name": "@extremexp/vs-code-extension",
    "version": "1.0.0",
    "description": "VS Code extension for extremexp",
    "main": "./dist/extension.js",
    "types": "./dist/extension.d.ts",
    "exports": {
        ".": {
            "types": "./dist/extension.d.ts",
            "default": "./dist/extension.js"
        }
    },
    "scripts": {
        "build": "tsc --build",
        "build:watch": "tsc --build --watch",
        "clean": "tsc --build --clean",
        "test": "echo \"No tests specified\" && exit 0",
        "vscode:prepublish": "npm run build",
        "package": "vsce package"
    },
    "files": [
        "dist/**/*",
        "src/**/*"
    ],
    "keywords": [
        "extremexp",
        "vscode",
        "extension"
    ],
    "author": "",
    "license": "MIT",
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extremexp.helloWorld"
    ],
    "contributes": {
        "commands": [
            {
                "command": "extremexp.helloWorld",
                "title": "Hello World",
                "category": "ExtremEXP"
            }
        ]
    },
    "dependencies": {
        "@extremexp/core": "^1.0.0",
        "@extremexp/artifact-generator": "^1.0.0"
    },
    "devDependencies": {
        "typescript": "^5.4.5",
        "@types/vscode": "^1.85.0",
        "@vscode/vsce": "^2.24.0"
    }
}