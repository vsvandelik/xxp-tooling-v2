=== SimpleWorkflow.xxp ===
workflow SimpleWorkflow {
    define task myTask;
    define task anotherTask;
    
    configure task myTask {}

    START -> myTask -> anotherTask -> END;
}

=== experiment.espace ===
experiment MyExperiment {
    space s1 of SimpleWorkflow {
        strategy randomSearch;
        
        configure task ###myTask### {
            param threshold = 0.5;
        }
    }
    
    control {
        start -> s1 -> end;
    }
}

