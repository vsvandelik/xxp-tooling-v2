{
    "name": "@extremexp/workflow-repository",
    "version": "1.0.0",
    "description": "Client library for managing workflows in ExtremeXP",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist/**/*",
        "src/**/*"
    ],
    "scripts": {
        "build": "tsc --build",
        "build:watch": "tsc --build --watch",
        "clean": "tsc --build --clean",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "keywords": [
        "extremexp",
        "workflow",
        "repository",
        "client"
    ],
    "author": "Vojtech Svandelik",
    "license": "MIT",
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    },
    "dependencies": {
        "jszip": "^3.10.1"
    }
}
