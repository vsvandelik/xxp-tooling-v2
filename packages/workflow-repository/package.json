{
    "name": "@extremexp/workflow-repository-server",
    "version": "1.0.0",
    "description": "A repository for managing workflows in Extremexp",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module",
    "bin": {
        "experiment-runner": "./dist/server.js"
    },
    "exports": {
        ".": {
            "types": "./dist/server.d.ts",
            "default": "./dist/server.js"
        }
    },
    "scripts": {
        "build": "tsc --build",
        "build:watch": "tsc --build --watch",
        "clean": "tsc --build --clean",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "files": [
        "dist/**/*",
        "src/**/*"
    ],
    "keywords": [
        "extremexp",
        "artifact",
        "generator"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "express": "^4.18.2",
        "cors": "^2.8.5",
        "multer": "^1.4.5",
        "bcrypt": "^5.1.1",
        "jsonwebtoken": "^9.0.2",
        "jszip": "^3.10.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/cors": "^2.8.17",
        "@types/multer": "^1.4.11",
        "@types/bcrypt": "^5.0.2",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/node": "^20.12.7",
        "tsx": "^4.7.1",
        "typescript": "^5.8.3"
    }
}