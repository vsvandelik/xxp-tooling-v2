{
    "name": "@extremexp/experiment-runner",
    "version": "1.0.0",
    "description": "Artifact generation utilities for extremexp",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module",
    "bin": {
        "experiment-runner": "./dist/cli.js"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsc --build",
        "build:watch": "tsc --build --watch",
        "clean": "tsc --build --clean",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "files": [
        "dist/**/*",
        "src/**/*"
    ],
    "keywords": [
        "extremexp",
        "experiment",
        "runner"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@extremexp/core": "^1.0.0",
        "@types/sqlite3": "^3.1.11",
        "ora": "^8.2.0",
        "sqlite": "^5.1.1",
        "sqlite3": "^5.1.7"
    },
    "devDependencies": {
        "@types/prompts": "^2.4.9",
        "typescript": "^5.4.5"
    }
}
