=== runnerWorkflow.xxp ===

workflow RunnerWorkflow {
    define task task123;
    START -> task123 -> END;

    configure task task123 {
        implementation "script.py";
    }
}

=== runner.espace ===

experiment E1 {
    space ASpace of RunnerWorkflow {
        strategy gridsearch;
    }
    control {
        START -> ASpace -> END;
    }
}

